{
  "name": "Kruner Hero",
  "homepage_url": "https://github.com/skidlamer/krunker_hero/archive/master.zip",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "A krunker.io modification",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://krunker.io/*",
    "*://matchmaker.krunker.io/*",
    "background"
  ],
  "web_accessible_resources": [
    "zip.js", "zip-ext.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Krunker Hero"
  },
  "background": {
    "scripts": ["hook.js"]
  }
}
